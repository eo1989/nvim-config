{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "runtime.version": "LuaJIT",
    "diagnostics.globals": [
      "quarto",
      "pandoc",
      "bit",
      "io",
      "string",
      "print",
      "require",
      "table",
      "eo",
      "vim",
      "P",
      "describe",
      "it",
      "before_each",
      "after_each",
      "pending",
      "hs",
      "spoon",
      "map"
    ],
    "diagnostics.disable": [
      "missing-fields",
      "undefined-doc-param",
      "doc-field-no-class",
      "missing-parameter",
      "return-type-mismatch",
      "param-type-mismatch",
      "cast-local-type",
      "inject-field",
      "undefined-global",
      "duplicate-set-field",
      "assign-type-mismatch",
      "undefined-field",
        "need-check-nil",
        "unused-local",
        "undefined-doc-name",
        "redundant-return-value"
    ],
    "workspace.checkThirdParty": false
}
